# Environment Variables Example
# Copy this file to .env and update with your values

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=False

# Environment
ENVIRONMENT=production

# Database
DATABASE_PATH=./feedback.db

# CORS Origins (comma-separated)
# For production, specify your Streamlit Cloud URL
# Example: CORS_ORIGINS=https://your-app.streamlit.app,https://another-domain.com
CORS_ORIGINS=*
